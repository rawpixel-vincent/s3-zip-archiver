{
  "name": "s3-zip-archiver",
  "version": "1.0.0-alpha.2",
  "description": "Read and upload a zip archive of s3 files to s3 using streams",
  "main": "index.mjs",
  "types": "index.d.mts",
  "type": "module",
  "scripts": {
    "build": "npm run types:emit && npm run bundle:cjs && npm run bundle:esm",
    "bundle:cjs": "rm -rf dist/cjs || true && mkdir -p dist/cjs && esbuild --platform=node index.mjs utils.mjs --target=node18 --format=cjs --outdir=dist/cjs --out-extension:.js=.cjs",
    "bundle:esm": "rm -rf dist/esm || true && mkdir -p dist/esm && esbuild --platform=node index.mjs utils.mjs --target=node18 --format=esm --outdir=dist/esm --out-extension:.js=.mjs",
    "types:emit": "tsc --allowJs",
    "precommit": "lint-staged && npm run build && git add dist/.",
    "prepare": "husky install",
    "prettier": "prettier --write \"./*.js\"",
    "lint": "eslint . --ext .mjs --fix",
    "eslint": "eslint . --ext .mjs"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3",
    "@aws-sdk/lib-storage": "^3",
    "archiver": "^5"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3",
    "@aws-sdk/lib-storage": "^3",
    "@types/archiver": "^5",
    "@types/node": "^18.14.0",
    "archiver": "^5",
    "esbuild": "^0.17.9",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "terser": "^5.16.4",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint *.js --fix"
    ]
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "packageManager": "npm@9.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rawpixel-vincent/s3-zip-archiver.git"
  },
  "bugs": {
    "url": "https://github.com/rawpixel-vincent/s3-zip-archiver/issues"
  },
  "author": "vincent baronnet",
  "contributors": [],
  "license": "MIT",
  "keywords": [
    "s3",
    "zip",
    "archiver",
    "aws",
    "stream"
  ]
}
